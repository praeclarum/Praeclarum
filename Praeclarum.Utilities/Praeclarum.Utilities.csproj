<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net6.0;net6.0-ios;net6.0-maccatalyst</TargetFrameworks>

    <PackageId>Praeclarum.Utilities</PackageId>
    <Version>1.0.0</Version>
    <Authors>praeclarum</Authors>
    <PackageProjectUrl>https://github.com/praeclarum/Praeclarum</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>

    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <DefineConstants>NO_DROPBOX</DefineConstants>
  </PropertyGroup>

  <Import Project="..\Praeclarum.iOS\Praeclarum.iOS.Shared.projitems" Label="Shared" Condition="('$(TargetFramework)' == 'net6.0-ios' or '$(TargetFramework)' == 'net6.0-maccatalyst') and Exists('..\Praeclarum.iOS\Praeclarum.iOS.Shared.projitems')" />
  <Import Project="..\Praeclarum\Praeclarum.Shared.projitems" Label="Shared" Condition="Exists('..\Praeclarum\Praeclarum.Shared.projitems')" />
</Project>
